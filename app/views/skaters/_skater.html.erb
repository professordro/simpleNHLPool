<tr>
  <td><%= skater.name %></td>
  <td><%= skater.team %></td>
  <td><%= skater.pos %></td>
  <td><%= skater.goals %></td>
  <td><%= skater.assists %></td>
  <td>
    <%= form_tag("removeSkater", method: "post", remote: true) do %>
        <%= hidden_field_tag :skaterId, skater.id %>
        <%= hidden_field_tag :pool, skater.pool_member.pool.id %>
        <%= hidden_field_tag :id, skater.pool_member.id %>
        <%= submit_tag("Remove Skater") %>
    <% end %>
  </td>
</tr>